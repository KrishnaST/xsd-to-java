<xs:include schemaLocation="UPI-Common.xsd"></xs:include>

	<xs:element name="ReqBioAuth">
		<xs:annotation>
			<xs:documentation>This API Allows PSP to Request for BioAuth for a user.
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Head" type="upi:headType" />
				<xs:element name="Txn" type="upi:payTrans" />
				<xs:element name="Payer" type="upi:payerType" />
			</xs:sequence>
		</xs:complexType>		
	</xs:element>


	<xs:element name="RespBioAuth">
		<xs:annotation>
			<xs:documentation>This API Allows PSP to Request for Balance enquiry for
				a user.
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Head" type="upi:headType" />
				<xs:element name="Txn" type="upi:payTrans" />
				<xs:element name="Resp" type="upi:respType" />
				<xs:element name="UidaiData" type="upi:uidaiData" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
